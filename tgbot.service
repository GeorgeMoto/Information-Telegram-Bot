[Unit]
Description=Telegram Bot
After=network.target

[Service]
User=george
Group=george

WorkingDirectory=/home/george/Inforamtion-Telegtam-Bot/
Environment="PYTHONPATH=/home/george/Inforamtion-Telegtam-Bot/"
ExecStart=/home/george/Inforamtion-Telegtam-Bot/.venv/bin/python /home/george/Inforamtion-Telegtam-Bot/bot.py

[Install]
WantedBy=multi-user.target